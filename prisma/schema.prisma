generator client-js {
  provider = "prisma-client-js"
}

generator client-py {
  provider = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = "5"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alembic_version {
  version_num String @id(map: "alembic_version_pkc") @db.VarChar(32)
}

model Test {
  id   Int    @id
  name String @db.VarChar(255)
  
  @@map("tests")
}
